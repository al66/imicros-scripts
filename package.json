{
  "name": "imicros-scripts",
  "version": "0.0.2",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Moleculer service for running customer code (untrusted code)",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache",
    "test-scripts": "jest test/scripts.spec.js --coverage --no-cache",
    "test-json": "jest test/json4json.spec.js --coverage --no-cache"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js",
      "!lib/globals.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-scripts.git"
  },
  "keywords": [
    "imicros",
    "moleculer",
    "scripts",
    "untrusted code",
    "vm"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "imicros-acl": "0.0.3",
    "jest": "^24.8.0",
    "moleculer": "^0.13.9",
    "nats": "^1.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.13",
    "vm2": "^3.8.2"
  }
}
